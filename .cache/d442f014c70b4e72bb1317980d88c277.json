":2}},{"name":"PropTypes","generated":{"line":116,"column":12},"source":"src/components/Courses.js","original":{"line":71,"column":12}},{"name":"bool","generated":{"line":116,"column":32},"source":"src/components/Courses.js","original":{"line":71,"column":22}},{"generated":{"line":116,"column":36},"source":"src/components/Courses.js","original":{"line":71,"column":12}},{"name":"isRequired","generated":{"line":116,"column":37},"source":"src/components/Courses.js","original":{"line":71,"column":27}},{"generated":{"line":117,"column":0},"source":"src/components/Courses.js","original":{"line":70,"column":20}},{"generated":{"line":117,"column":1},"source":"src/components/Courses.js","original":{"line":70,"column":0}},{"name":"Courses","generated":{"line":119,"column":18},"source":"src/components/Courses.js","original":{"line":74,"column":15}}],"sources":{"src/components/Courses.js":"import React, { Component } from 'react'\nimport PropTypes from 'prop-types'\nimport CardCourse from './CardCourse'\nimport SearchCourse from './SearchCourse'\n\nclass Courses extends Component {\n  constructor (props) {\n    super(props)\n    this.state = {\n      showMenu: props.showMenu,\n      courses: JSON.parse(window.localStorage.getItem('cursos')),\n      searchedCourse: '',\n      foundCourses: []\n    }\n\n    this.changeStateCourse = this.changeStateCourse.bind(this)\n    this.searchCourse = this.searchCourse.bind(this)\n  }\n\n  componentWillReceiveProps (props) {\n    this.setState({showMenu: props.showMenu})\n  }\n\n  changeStateCourse (id) {\n    let x = Object.assign(this.state.courses)\n\n    x.find((course) => {\n      if (course.id === id) course.activo = !course.activo\n    })\n\n    this.setState({courses: x})\n    window.localStorage.setItem('cursos', JSON.stringify(x))\n  }\n\n  searchCourse (e) {\n    this.setState({\n      searchedCourse: e.target.value,\n      foundCourses: this.state.courses.filter(curso => {\n        if (e.target.value !== '') return curso.curso.toLowerCase().includes(e.target.value.toLowerCase())\n      })\n    })\n  }\n\n  render () {\n    let hiddenMenuStyles = {\n      top: '48px',\n      boxShadow: 'none'\n    }\n\n    let showMenuStyles = {\n      top: '144px',\n      boxShadow: '0 -3px 6px rgba(0, 0, 0, 0.4)'\n    }\n\n    return (\n      <main className='courses' style={this.state.showMenu ? showMenuStyles : hiddenMenuStyles} >\n        <SearchCourse searchCourse={this.searchCourse} />\n        <div className='courses__container'>\n          {\n            this.state.foundCourses.length > 0\n              ? this.state.foundCourses.map((course) => (<CardCourse changeStateCourse={this.changeStateCourse} key={course.id} course={course} />))\n              : this.state.courses.map((course) => (<CardCourse changeStateCourse={this.changeStateCourse} key={course.id} course={course} />))\n          }\n        </div>\n      </main>\n    )\n  }\n}\n\nCourses.propTypes = {\n  showMenu: PropTypes.bool.isRequired\n}\n\nexport default Courses\n"},"lineCount":null}},"hash":"600b37c613d311f661bb06771e3b533f","cacheData":{"env":{}}}